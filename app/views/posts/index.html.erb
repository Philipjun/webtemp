<h1>Listing posts</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th colspan="2"></th>
  </tr>

  <%= @posts.each do |p| %>
    <tr>
      <td><%= p.title %></td>
      <td><%= p.text %></td>
      <td><%= link_to "Show", post_path(post) %></td>
      <td><%= link_to "Edit Post", edit_post_path(post) %></td>
    </tr>
  <% end %>
</table>

